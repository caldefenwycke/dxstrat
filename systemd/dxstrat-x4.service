[Unit]
Description=dxstrat x4-lane pool
After=network.target bitcoind.service

[Service]
WorkingDirectory=/opt/darwinx/dxstrat/engine/x4pool
Environment=PYTHONUNBUFFERED=1
ExecStart=/usr/bin/python3 -m src.dxstrat.stratum_server
Restart=always
RestartSec=2
User=root

[Install]
WantedBy=multi-user.target
